{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "akaunting/language": "^1.0",
        "backpack/crud": "^4.0.0",
        "backpack/permissionmanager": "^5.0",
        "curl/curl": "^2.2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "kkomelin/laravel-translatable-string-exporter": "^1.6",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mailerlite/mailerlite-api-v2-php-sdk": "^0.2.3",
        "phpoffice/phpword": "^0.16.0",
        "sentry/sentry-laravel": "^1.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-query-builder": "^2.8",
        "spatie/laravel-translatable": "^4.2",
        "spatie/laravel-uptime-monitor": "^3.8",
        "symfony/psr-http-message-bridge": "^1.1",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "backpack/generators": "^2.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "laravel/dusk": "^5.11",
        "laravel/telescope": "^2.1",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.42",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
